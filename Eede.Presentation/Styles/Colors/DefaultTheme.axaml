<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <ResourceDictionary.ThemeDictionaries>
      <ResourceDictionary x:Key='Light'>
        <SolidColorBrush x:Key='RegionBrush'>#FFFFFF</SolidColorBrush>
        <SolidColorBrush x:Key='BaseBrush'>#CCCCCC</SolidColorBrush>
        <SolidColorBrush x:Key='PrimaryBrush'>#0073CF</SolidColorBrush>
      </ResourceDictionary>
      <ResourceDictionary x:Key='Dark'>
        <SolidColorBrush x:Key='RegionBrush'>Black</SolidColorBrush>
        <SolidColorBrush x:Key='BaseBrush'>#333333</SolidColorBrush>
        <SolidColorBrush x:Key='PrimaryBrush'>#0073CF</SolidColorBrush>
      </ResourceDictionary>
    </ResourceDictionary.ThemeDictionaries>
</ResourceDictionary>
