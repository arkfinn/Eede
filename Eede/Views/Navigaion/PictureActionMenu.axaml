<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:local="clr-namespace:Eede.Views.Navigaion"
             xmlns:buttons="clr-namespace:Eede.Views.General.Buttons"
             xmlns:p="clr-namespace:Eede.Common.Pictures.Actions"
             mc:Ignorable="d"
             d:DesignHeight="450" d:DesignWidth="24"
             x:Class="Eede.Views.Navigaion.PictureActionMenu"
             >
  <Grid>
    <StackPanel Orientation="Vertical" >
      <buttons:ToolButton
        Source="/Assets/tool_shift_up.png"
        Command="{ Binding Command, RelativeSource={ RelativeSource AncestorType={ x:Type local:PictureActionMenu } } }"
        CommandParameter="{ x:Static p:PictureActions.ShiftUp }"
      />
    </StackPanel>
  </Grid>
</UserControl>
